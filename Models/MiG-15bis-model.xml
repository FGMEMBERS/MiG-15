		<type>rotate</type>
		<object-name>TP_ElevatorL</object-name>
		<object-name>TP_Trimmer</object-name>
		<property>fdm/jsbsim/fcs/elevator-indicated-pos-rad</property>
		<factor>-57.29</factor>
		<center>
			<x-m>9.16</x-m>
