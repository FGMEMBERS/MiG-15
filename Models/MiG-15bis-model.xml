		</center>
		<axis>
			<y>1</y>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>PLT_RShin</object-name>
		<property>instrumentation/pedals/rudder</property>
		<factor>-10</factor>
		<center>
			<x-m>1.333</x-m>
			<z-m>-0.057</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
	</animation>



<!-- Elevator -->
