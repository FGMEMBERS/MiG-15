<!-- Rudder -->
  <animation>
    <type>rotate</type>
    <object-name>TF_Rudder</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>10</factor>
    <center>
      <x-m>8</x-m>
      <y-m>0</y-m>
      <z-m>0.5</z-m>
    </center>
    <axis>
      <x>0.874</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

<!-- Pilot Move Left Pedal -->

	<animation>
		<type>translate</type>
		<object-name>PLT_LShin</object-name>
		<property>instrumentation/pedals/shift_x</property>
		<factor>1</factor>
		<axis>
			<x>-1</x>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>PLT_LThigh</object-name>
		<property>instrumentation/pedals/rudder</property>
		<factor>10</factor>
		<center>
			<x-m>2.059</x-m>
			<z-m>-0.040</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>PLT_LShin</object-name>
		<property>instrumentation/pedals/rudder</property>
		<factor>-10</factor>
		<center>
			<x-m>1.333</x-m>
			<z-m>-0.057</z-m>
		</center>
		<axis>
			<y>-1</y>
		</axis>
	</animation>

<!-- Right pedal -->

	<animation>
		<type>translate</type>
		<object-name>PLT_RShin</object-name>
		<property>instrumentation/pedals/shift_x</property>
		<factor>1</factor>
		<axis>
			<x>1</x>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>PLT_RThigh</object-name>
		<property>instrumentation/pedals/rudder</property>
		<factor>10</factor>
