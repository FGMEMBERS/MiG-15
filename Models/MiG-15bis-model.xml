<!-- Landing Gears -->
<!-- Front -->
<!-- Show or hide u/c and doors -->
	<animation>
		<type>select</type>
		<object-name>FUS_Nose_WWell</object-name>
		<condition>
			<or>
				<greater-than>
					<property>fdm/jsbsim/gear/unit[0]/pos-norm-real</property>
					<value>0.0</value>
				</greater-than>
				<greater-than>
					<property>fdm/jsbsim/gear/unit[0]/tored</property>
					<value>0.0</value>
				</greater-than>
			</or>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>FUS_NoseDoorLB</object-name>
		<object-name>FUS_NoseDoorRB</object-name>
		<object-name>UC_Nose_MainPost</object-name>
		<object-name>UC_Nose_ArmHub</object-name>
		<object-name>UC_Nose_Axle</object-name>
		<object-name>UC_Nose_Tyre</object-name>
		<condition>
			<greater-than>
				<property>fdm/jsbsim/gear/unit[0]/pos-norm-real</property>
				<value>0.0</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>FUS_NoseDoorLS</object-name>
		<object-name>FUS_NoseDoorRS</object-name>
		<condition>
			<less-than>
				<property>fdm/jsbsim/gear/unit[0]/tored</property>
				<value>1.0</value>
			</less-than>
		</condition>
	</animation>

<!-- Extend/Retract gear -->
	<animation>
		<type>rotate</type>
		<object-name>UC_Nose_MainPost</object-name>
		<object-name>UC_Nose_ArmHub</object-name>
		<object-name>UC_Nose_Axle</object-name>
		<object-name>UC_Nose_Tyre</object-name>
		<property>fdm/jsbsim/gear/unit[0]/pos-norm-real</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>-105.0</dep></entry>
			<entry><ind>0.5</ind><dep>-105.0</dep></entry>
			<entry><ind>1.0</ind><dep>0.0</dep></entry>
		</interpolation>
		<center>
			<x-m>1.18</x-m>
			<y-m>0.00</y-m>
			<z-m>-0.56</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

<!-- Open/Close doors -->
<!-- Left Door -->
	<animation>
		<type>rotate</type>
		<object-name>FUS_NoseDoorLS</object-name>
		<object-name>FUS_NoseDoorLB</object-name>
		<property>fdm/jsbsim/gear/unit[0]/pos-norm-real</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>0.5</ind><dep>-95.0</dep></entry>
			<entry><ind>1.0</ind><dep>-95.0</dep></entry>
		</interpolation>
		<center>
			<x-m>0.23</x-m>
			<y-m>-0.14</y-m>
			<z-m>-0.43</z-m>
		</center>
		<axis>
			<x>1.0</x>
			<y>0.0</y>
			<z>-0.216</z>
		</axis>
	</animation>

<!-- Right Door -->
	<animation>
		<type>rotate</type>
		<object-name>FUS_NoseDoorRS</object-name>
		<object-name>FUS_NoseDoorRB</object-name>
		<property>fdm/jsbsim/gear/unit[0]/pos-norm-real</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>0.5</ind><dep>95.0</dep></entry>
			<entry><ind>1.0</ind><dep>95.0</dep></entry>
		</interpolation>
			<center>
				<x-m>0.23</x-m>
				<y-m>0.14</y-m>
				<z-m>-0.43</z-m>
			 </center>
			<axis>
				<x>1.0</x>
				<y>0.0</y>
				<z>-0.216</z>
			</axis>
	</animation>


<!-- Steering -->
	<animation>
		<type>rotate</type>
		<object-name>UC_Nose_MainPost</object-name>
		<object-name>UC_Nose_ArmHub</object-name>
		<object-name>UC_Nose_Axle</object-name>
		<object-name>UC_Nose_Tyre</object-name>
		<property>gear/gear[0]/steering-norm</property>
		<offset-deg>0</offset-deg>
		<factor>-45</factor>
		<center>
			<x-m>1.07</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.6</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>

<!-- Compression -->
	<animation>
		<type>rotate</type>
		<object-name>UC_Nose_ArmHub</object-name>
		<object-name>UC_Nose_Axle</object-name>
		<object-name>UC_Nose_Tyre</object-name>
		<property>gear/gear[0]/compression-norm</property>
		<factor>-70</factor>
