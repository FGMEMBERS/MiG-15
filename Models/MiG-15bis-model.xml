<!-- Flaps -->

<!-- Check if tored -->

	<animation>
		<type>select</type>
		<object-name>WNG_FlapLUp</object-name>
		<object-name>WNG_FlapLDown</object-name>
		<object-name>WNG_FlapRUp</object-name>
		<object-name>WNG_FlapRDown</object-name>
		<condition>
			<equals>
				<property>fdm/jsbsim/fcs/flap-tored</property>
				<value>0</value>	
			</equals>
		</condition>
	</animation>

<!-- Left -->
	<animation>
		<type>translate</type>
		<object-name>WNG_FlapLUp</object-name>
		<object-name>WNG_FlapLDown</object-name>
		<property>fdm/jsbsim/fcs/flap-pos-norm</property>
		<factor>0.25</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>-0.01</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>WNG_FlapLUp</object-name>
		<object-name>WNG_FlapLDown</object-name>
		<property>fdm/jsbsim/fcs/flap-pos-norm</property>
		<factor>-50</factor>
		<center>
			<x-m>4.32</x-m>
			<y-m>-1.04</y-m>
			<z-m>-0.08</z-m>
		</center>
		<axis>
